{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
              "@components": "./src/components",
              "@containers": "./src/containers",
              "@pages": "./src/pages",
              "@styles": "./src/styles",
              "@assets": "./src/assets",
              "@hooks": "./src/hooks",
              "@features": "./src/features",
              "@navigation": "./src/navigation",
            }
          }]
    ]
}
